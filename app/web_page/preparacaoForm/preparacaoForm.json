        "\t\t\t\tcliente: $data.encomenda.produto.cliente !== undefined ? $data.encomenda.produto.cliente : null,",
        "\t\t\t\tlocalEntrega: $data.encomenda.produto.localEntrega !== undefined ? $data.encomenda.produto.localEntrega : null,",
        "\t\t\t\testaProntoParaEntrega: $data.encomenda.produto.estaProntoParaEntrega !== undefined ? $data.encomenda.produto.estaProntoParaEntrega : null",
        "\t\t\t} : null",
        "\t\t}",
