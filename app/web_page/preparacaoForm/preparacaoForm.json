    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.encomenda && $data.encomenda_produto ){",
        "\t//attach lazy references variables to parent variables",
        "\t$data.encomenda.produto = $data.encomenda_produto;",
        "\treturn {",
        "\t\t//map encomenda variable to expected task contract input",
        "\t\tencomendaInput: {",
        "\t\t\tproduto: $data.encomenda.produto ? {",
        "\t\t\t\tpersistenceId_string: $data.encomenda.produto.persistenceId_string !== undefined ? $data.encomenda.produto.persistenceId_string : null,",
        "\t\t\t\tdescricao: $data.encomenda.produto.descricao !== undefined ? $data.encomenda.produto.descricao : null,",
